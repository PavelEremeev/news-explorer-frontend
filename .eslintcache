ponents/Preloader/Preloader';\n\nfunction SavedNews(\n  {\n    isLoading,\n    loggedIn,\n    onCardDelete,\n    onGetCards,\n    savedCards,\n  }) {\n\n  React.useEffect(() => {\n    onGetCards();\n  }, []);\n\n  return (\n    <section className=\"saved-news\">\n      <div className=\"saved-news__container\">\n        {isLoading ? <Preloader/> : savedCards.map((card) => (\n          <NewsCard key={card._id}\n                    {...card}\n                    loggedIn={loggedIn}\n                    onCardDelete={onCardDelete}\n          />))\n        }\n      </div>\n    </section>\n  );\n}\n\nexport default SavedNews;\n","C:\\Users\\Eremeev\\Desktop\\praktikum\\dev\\news-explorer-frontend\\src\\contexts\\CurrentUserContext.js",[],"C:\\Users\\Eremeev\\Desktop\\praktikum\\dev\\news-explorer-frontend\\src\\components\\NewsCard\\NewsCard.js",[],"C:\\Users\\Eremeev\\Desktop\\praktikum\\dev\\news-explorer-frontend\\src\\components\\SavedNewsHeader\\SavedNewsHeader.js",["118"],"import React from 'react';\nimport './SavedNewsHeader.css';\nimport Navigation from '../../components/Navigation/Navigation';\nimport {CurrentUserContext} from \"../../contexts/CurrentUserContext\";\n\nfunction SavedNewsHeader(\n  { loggedIn, titles, onSignIn, onSignOut, isPopupOpen, savedCards }) {\n\n  const currentUser = React.useContext(CurrentUserContext);\n  const [category, setCategory] = React.useState([]);\n  const [extension, setExtension] = React.useState('');\n  const [numArticles, setNumArticles] = React.useState('');\n\n  React.useEffect(() => {\n    function getPopularCategory() {\n      const categoryCountObj = savedCards.reduce((acc, cur) => {\n        if (acc[cur.keyword]) {\n          acc[cur.keyword] += 1;\n        } else {\n          acc[cur.keyword] = 1;\n        }\n        return acc;\n      }, {});\n      const arrNames = Object.keys(categoryCountObj).sort((a, b) => {\n        return categoryCountObj[b] - categoryCountObj[a];\n      });\n      setCategory(arrNames);\n    }\n\n    getPopularCategory();\n  }, [savedCards]);\n\n  React.useEffect(() => {\n    const getNumbersExtensions = (n, titles) => {\n      return titles[\n        (n % 10 === 1 && n % 100 !== 11 ? 0 : n % 10 >= 2 && n % 10 <= 4 && (n % 100 < 10 || n % 100 >= 20) ? 1 : 2)\n        ];\n    };\n\n    setExtension(getNumbersExtensions(\n      category.length - 2,\n      ['-ой другой', '-м другим', '-и другим']\n    ));\n\n    setNumArticles(getNumbersExtensions(\n      savedCards.length,\n      ['сохраненная статья', 'сохраненные статьи', 'сохраненных статей']\n    ));\n  }, [category]);\n\n  return (\n    <header className=\"saved-news-header\">\n      <Navigation\n        loggedIn={loggedIn}\n        onSignIn={onSignIn}\n        onSignOut={onSignOut}\n        theme={'saved-news'}\n        isPopupOpen={isPopupOpen}\n      />\n      <div className=\"saved-news-header__container\">\n        <p className=\"saved-news-header__text\">Сохранённые статьи</p>\n        <h2 className=\"saved-news-header__title\">\n          {`${currentUser.name}, у вас ${savedCards.length} ${numArticles}`}\n        </h2>\n        {category.length > 0 &&\n        <p className=\"saved-news-header__subtitle\">\n          По ключевым словам:\n          <span className=\"saved-news-header__span-accent\"> {category[0] || ''}</span>\n          {category.length > 1 && ','}\n          <span className=\"saved-news-header__span-accent\"> {category[1] || ''}</span>\n          {category.length >= 3 && ' и'}\n          {category.length === 3 &&\n          <span className=\"saved-news-header__span-accent\"> {category[2]}</span>}\n          {category.length > 3 &&\n          <span className=\"saved-news-header__span-accent\"> {`${category.length - 2}${extension}`}</span>\n          }\n        </p>\n        }\n      </div>\n    </header>\n  );\n}\n\nexport default SavedNewsHeader;\n","C:\\Users\\Eremeev\\Desktop\\praktikum\\dev\\news-explorer-frontend\\src\\utils\\constants.js",[],"C:\\Users\\Eremeev\\Desktop\\pra